	<xs:complexType name="mainGearGlobalType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Global description of main landing gear.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>MainGearGlobal type, containing global parameters of a main landing gear.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="totalLength" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Total length of main landing gear, equals the distance form the middle of the bogie/axles to the middle of the pintle strut. Distance is measured while main landing gear is fully extended.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="retractAngle" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Retraction angle of the main landing gear. Equals a rotation around the global z-axis in degrees. 0 = retraction to the front; 90 = retraction to the left; 180 = retraction to the rear; 270 = retraction to the right.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="mainGearSupportBeamPositionType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Definition of the main landing gear support beam position.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Definition of the main landing gear support beam position.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="xsiInside" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Relative chordwise coordinate (xsi) of the inner end of the support beam. Xsi refers to the componentSegment where the landing gear is attached to. The eta postion of the inner end is defined by the eta position of the wing root (=wing-fuselage attachment).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="etaOutside" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Relative spanwise coordinate (eta) of the outer end of the support beam. Eta refers to the componentSegment where the landing gear is attached to. The xsi coordinate of the outer end is defined by the spar position (first spar), where the support beam is attached to.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="mainGearSupportBeamType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Definition of the main landing gear support beam.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Definition of the main landing gear support beam, if a support beam is used for the attachment. The defintion includes cross section properties as well as the position of the support beam.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="position" type="mainGearSupportBeamPositionType" />
					<xs:element name="structure" type="sparCrossSectionType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="mainGearType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Main landing gear.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>MainGear type, containing all data off one single main landing gear.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of the main landing gear.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Description of the main landing gear.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>UID of the part, where the landing gear is mounted. If the gear is attached to the wing, the corresponding componentSegment is the parent. If the gear is attached to the fuselage, the fuselage is the parent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="wingAttachment" type="mainGearWingAttachmentType" />
					<xs:element minOccurs="0" name="fuselageAttachment" type="transformationType" />
					<xs:element minOccurs="0" name="global" type="mainGearGlobalType" />
					<xs:element minOccurs="0" name="components" type="gearComponentsType" />
				</xs:all>
				<xs:attribute name="uID" type="xsd:string" use="required" />
				<xs:attribute name="symmetry" use="optional">
					<xs:simpleType>
						<xs:restriction base="xsd:string">
							<xs:enumeration value="x-y-plane" />
							<xs:enumeration value="x-z-plane" />
							<xs:enumeration value="y-z-plane" />
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="mainGearWingAttachmentPositioningType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Definition of the position of the landing gear.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Definition of the position of the landing gear (intersection point of main strut and pintle sturt) on the wing, using relative componentSegment coordinates.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="relHeight" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Relative height of the landing gear attachment point (intersection point of main strut and pintle sturt), relative to the airfoil hight at the attachment position.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="eta" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Relative spanwise position (eta) of the landing gear attachment point (intersection point of main strut and pintle sturt). Eta refers to the componentSegment where it is attached.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice>
						<xs:element name="xsi" type="doubleBaseType">
							<xs:annotation>
								<xs:documentation>Relative chordwise position (xsi) of the landing gear attachment point (intersection point of main strut and pintle sturt). Xsi refers to the componentSegment where it is attached.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="positionOverlap" type="xsd:string" />
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="mainGearWingAttachmentType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Definition of the wing attachment.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Definition of the wing attachment, if attached to the wing. The definition includes the position of the landing gear as well as the information to which spars resp. supportBeam the gear is attached.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="firstSparUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>UID of the first spar, where the landing gear is attached to.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="positioning" type="mainGearWingAttachmentPositioningType" />
					<xs:choice>
						<xs:element minOccurs="0" name="secondSparUID" type="stringUIDBaseType">
							<xs:annotation>
								<xs:documentation>UID of the second spar, where the landing gear is attached to. Only used, if the landing gear is attached between two spars.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element minOccurs="0" name="supportBeam" type="mainGearSupportBeamType" />
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


