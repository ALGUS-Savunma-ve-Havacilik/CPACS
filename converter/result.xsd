					<xs:element minOccurs="0" name="positionXUidStart" type="stringUIDBaseType" />
					<xs:element minOccurs="0" name="positionXUidEnd" type="stringUIDBaseType" />
					<xs:element minOccurs="0" name="positionXUidMin" type="stringUIDBaseType" />
					<xs:element minOccurs="0" name="positionXUidMid" type="stringUIDBaseType" />
					<xs:element minOccurs="0" name="positionXUidMax" type="stringUIDBaseType" />
