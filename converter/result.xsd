	<xs:complexType name="materialType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>materialType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Material type, conatining data of a material Material orientation: 1: orientation in first preferred direction 2: orientation in second preferred direction 3: remaining orthogonal direction other stiffness components are dependent on the given ones by symmetry relationships. 3 different material behaviours can be defined which is carried out by the 3 branches of the definition.</ns2:para>
						<ns2:list>
							<ns2:listItem>isotropic material like aluminium and steel may be defined</ns2:listItem>
							<ns2:listItem>transversal isotropic material like uni-directional layers of CFRP may be defined</ns2:listItem>
							<ns2:listItem>orthotrop material like several layers of CFRP may be defined</ns2:listItem>
						</ns2:list>
						<ns2:para>For each material behaviour the minimum number of stiffness parameters are requested. All other stiffness components are dependent by material symmetry relationships.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of material</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>description of material</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="rho" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Density of material in [kg/mm^3]</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="k11" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>stiffness component in 11-direction in [N/m]</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="k12" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>stiffness component in 12-direction in [N/m]</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="1">
						<xs:annotation>
							<xs:documentation>choice between 3 different material behaviour</xs:documentation>
						</xs:annotation>
						<xs:sequence>
							<xs:annotation>
								<xs:documentation>mechanical properties for isotropic material other stiffness components are dependent on the given ones by symmetry relationships</xs:documentation>
							</xs:annotation>
							<xs:element name="sig11" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tau12" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>shear strength in 12 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="0" name="sig11yieldT" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile yield strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element maxOccurs="1" minOccurs="0" name="sig11yieldC" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>compressive yield strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:sequence>
							<xs:annotation>
								<xs:documentation>mechanical properties for transverse isotropic material Material orientation: 1: orientation in fiber direction 2 and 3: orientation orthogonal to fiber direction other stiffness components are dependent on the given ones by symmetry relationships</xs:documentation>
							</xs:annotation>
							<xs:element name="k22" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 22-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k23" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 23-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k66" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 66-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig11t" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig11c" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>compressive strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig22t" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile strength in 22 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig22c" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>compressive strength in 22 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tau12" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>shear strength in 12 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tau23" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>shear strength in 23 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
						<xs:sequence>
							<xs:annotation>
								<xs:documentation>mechanical properties for orthotropic material Material orientation: 1: orientation in first preferred direction 2: orientation in second preferred direction 3: remaining orthogonal direction</xs:documentation>
							</xs:annotation>
							<xs:element name="k13" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 13-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k22" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 22-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k23" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 23-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k33" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 33-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k44" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 44-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k55" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 55-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="k66" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>stiffness component in 66-direction</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig11t" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig11c" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>compressive strength in 11 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig22t" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile strength in 22 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig22c" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>compressive strength in 22 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig33t" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>tensile strength in 33 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="sig33c" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>compressive strength in 33 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tau12" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>shear strength in 12 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tau13" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>shear strength in 13 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="tau23" type="doubleBaseType">
								<xs:annotation>
									<xs:documentation>shear strength in 23 direction in [N/m^2]</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:choice>
					<xs:element maxOccurs="1" minOccurs="0" name="maxStrain" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Maximum strain of the material. This value is intended for layerd material. Thus no orientation is given.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="fatigueFactor" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>optional knockdown factor for fatiuqe (defaults to 1)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="postFailure" type="postFailureType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


