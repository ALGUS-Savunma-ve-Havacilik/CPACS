						<xs:annotation>
							<xs:documentation>Turbine entry total temperature</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="eiCO" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Emission index Carbon Monoxide</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="eiNOx" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Emission index Nitrogen Oxide</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="eiSoot" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Emission index Soot</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="eiHC" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Emission index unburned hydrocarbon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="rho8" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>air density at core outlet 8</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="rho18" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>air density at bypass outlet 18</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="nozzA8" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>area at core outlet</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="nozzA18" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>area at bypass outlet</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
				<xs:attribute name="uID" type="xsd:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


