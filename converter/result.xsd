	<xs:complexType name="delisProfileSizingSettingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>delisProfileSizingSettingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:annotation>
					<xs:documentation>Settings to define the sizing settings of beam/profile-like structures. Stringers that are smeared into the skin are not considered</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="stiffeningType">
						<xs:annotation>
							<xs:documentation>Choice determining the crosssection of the profile. The names and corresponding geometry is analog to the CPACS &#8220;standardProfileType Element&#8221;. An illustration is given in the CPACS documenation</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="T" />
									<xs:enumeration value="Z" />
									<xs:enumeration value="I" />
									<xs:enumeration value="J" />
									<xs:enumeration value="C" />
									<xs:enumeration value="L" />
									<xs:enumeration value="HAT" />
									<xs:enumeration value="CAP" />
									<xs:enumeration value="TUBE" />
									<xs:enumeration value="BOX" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="designCriteria" type="delisDesignCriteriaType" />
					<xs:element maxOccurs="1" minOccurs="0" name="skinMinimalThickness" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Minimal thickness of a sheet(total stacking thickness) Defaults to 0.001</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="delisSheetSizingSettingsType">
		<!--****************************************************
--><xs:annotation>
