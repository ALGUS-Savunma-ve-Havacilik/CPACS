	<xs:complexType name="globalAeroPerformanceType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>airfoilAeroPerformanceType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>airfoilAeroPerformance type, containing perfomance maps with aerodynamic data of an airfoil.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element minOccurs="0" name="reference" type="referenceType">
						<xs:annotation>
							<xs:documentation>References used for the calculation of the force and moment coefficients</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="aeroPerformanceMaps" type="aeroPerformanceMapsType">
						<xs:annotation>
							<xs:documentation>Calculated aerodynamic performance maps of the full configuration</xs:documentation>
