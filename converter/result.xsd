							<xs:documentation>Number of iterations for simulation of boarding / deboarding (0 = no simulation)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="simSetting">
						<xs:annotation>
							<xs:documentation>Setting of the simulation (default: all)</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="all" />
									<xs:enumeration value="full_Sorted" />
									<xs:enumeration value="full_Random" />
									<xs:enumeration value="3/4_Sorted" />
									<xs:enumeration value="3/4_Random" />
									<xs:enumeration value="1/2_Sorted" />
									<xs:enumeration value="1/2_Random" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


