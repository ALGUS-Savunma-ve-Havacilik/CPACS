	<xs:complexType name="wingWingAttachmentType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>wingWingAttachmentType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Definition of the structural connection between two wings resp. two componentSegments. Note: All structural connections between two wings/componetSegments have to be defined using wingWingAttachments. The wingWingAttachment has only be defined in one of the two componentSegments, that are connected.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
