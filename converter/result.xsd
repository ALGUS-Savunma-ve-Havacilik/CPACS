					<xs:element minOccurs="0" name="thrust" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Absolute thrust [N]</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="mDotFuel" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Fuel mass flow</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="v8" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Speed at core engine nozzle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="t8" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Total temperature at core engine nozzle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="mDot8" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Mass flow through core engine nozzle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="v18" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Speed at bypass nozzle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="t18" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Total temperature at bypass nozzle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="mDot18" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Mass flow through bypass nozzle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="n1" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Percent of n1Max, shaft 1</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="n2" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Percent of n2Max, shaft 2</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="piFan" type="stringVectorBaseType">
