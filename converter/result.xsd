					</xs:element>
					<xs:element name="origin">
						<xs:annotation>
							<xs:documentation>ICAO-Callsign of departure airport</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="ESKN" />
									<xs:enumeration value="EDFH" />
									<xs:enumeration value="EDDL" />
									<xs:enumeration value="EDDF" />
									<xs:enumeration value="EDDT" />
									<xs:enumeration value="ESSA" />
									<xs:enumeration value="EDVE" />
									<xs:enumeration value="EGSS" />
									<xs:enumeration value="EGLL" />
