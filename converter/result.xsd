			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of composite</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Description of composite</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="offset" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>offset of the laminate. The reference plane of the laminate is the arithmetic mean of the laminate thickness.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="unbounded" minOccurs="0" name="compositeLayer" type="compositeLayerType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


