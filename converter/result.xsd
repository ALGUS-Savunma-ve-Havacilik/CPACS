	<xs:complexType name="geogenWingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenWingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Elements of type geogenWingsType contain definitions of wing assemblies which should be generated by GeoGen.</ns2:para>
