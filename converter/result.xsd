	<xs:complexType name="gearDeflectionVectorType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>This type contains a vector of deflection values for a single gear</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:section>
								<ns2:title>0. General overview</ns2:title>
								<ns2:content>
									<ns2:para>In this type, a vector of deflections of a single gear is specified.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>1. &lt;gearUID&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>A reference to a gear from the aircraft model</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>2. &lt;relDeflection&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>A vector of relative deflections of the selected gear</ns2:para>
								</ns2:content>
							</ns2:section>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="gearUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Reference to a gear</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="relDeflection" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Relative deflections of the gear</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="gearDeflectionVectorsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>This type contains a list of gears and their deflection vectors</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:section>
								<ns2:title>0. General overview</ns2:title>
								<ns2:content>
									<ns2:para>In this type, a list of gears is defined.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>1. &lt;gear&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>One of these nodes per deflected gear is required here.</ns2:para>
								</ns2:content>
							</ns2:section>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
