	<xs:complexType name="delisWingModelSettingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>delisWingModelSettingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:annotation>
					<xs:documentation>Settings that affect the wing finite element model.</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfElementsInEtaDirectionInEachBay" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of finite elements in span(eta) direction within each region of adjacent spars and ribs. Defaults to 3</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfElementsInXsiDirectionInEachBay" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of finite elements in chord(xsi) direction within each region of adjacent spars and ribs. Defaults to 10</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="skinBaysInSizingRegionEta" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of bays in span(eta) direction that defines the size of the optimization region. Defaults to 1</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="skinBaysInSizingRegionXsi" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of bays in chord(xsi) direction that defines the size of the optimization region. Defaults to the number of bays between adjacent spars</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="createHalfWing" type="booleanBaseType">
						<xs:annotation>
							<xs:documentation>Flag if a half wing model should be created even if symmetry is given in the wing definition Defaults to False</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="createWingBox" type="booleanBaseType">
						<xs:annotation>
							<xs:documentation>Flag if just the wing box(region from front spar to rear spar) should be modeled. Otherwise a model from leading to trailing edge is created. Defaults to True</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="createControlSurfaces" type="booleanBaseType">
						<xs:annotation>
							<xs:documentation>Flag if the control surfaces should be modeled explicitly instead of using mass points Defaults to False</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="stringerModeling">
						<xs:annotation>
							<xs:documentation>Determines how stringers are modeled. These strings can be given: [smeared, simpleBeamSection, complexBeamSection] Defaults to smeared</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="smeared" />
									<xs:enumeration value="simpleBeamSection" />
									<xs:enumeration value="complexBeamSection" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="delisWingSettingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>delisWingSettingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:annotation>
					<xs:documentation>Wing model and sizing settings. If items are not specified, the default values are used.</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="1" name="wingUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>ID of the wing</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="modelSettings" type="delisWingModelSettingsType">
						<xs:annotation>
							<xs:documentation>Settings that affect the wing&#8217;s finite element model. The splitting of the optimization regions still account for the given splitting in CPACS. So if some certain structure is split more fine, while these parameters are coarse, the fine split stays intact!</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="sizingSettings" type="delisWingSizingSettingsType">
						<xs:annotation>
							<xs:documentation>Each structure element listed can be modified in terms of sizing properties.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

