	<xs:complexType name="compositeType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>compositeType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Composite type, conatining data of a composite</ns2:para>
