					<xs:element name="type">
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="toilet" />
									<xs:enumeration value="kitchen" />
									<xs:enumeration value="miscellaneous" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="x" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>forward center coordinate of object relative to cabin origin.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="y" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>center y-coordinate relative to cabin origin.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="length" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>length of the object</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="width" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>length of the object</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="height" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>length of the object</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="number" type="doubleBaseType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="cabinFloorElementsType">
		<!--****************************************************
--><xs:annotation>
