				<xs:all>
					<xs:element minOccurs="0" name="wingUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Reference to a wing; required, if separate paneling for each wing is used. If missing, paneling is the same for all wings.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="spanwise" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Spanwise paneling for each wing</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="chordwise" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Chordwise paneling for each wing</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="wingTip" type="vSAeroWingTipPanelingType">
						<xs:annotation>
							<xs:documentation>Paneling and secCurv for wing tip</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
				<xs:attribute name="uID" type="xsd:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseWingPanelingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseWingPanelingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroFuseWingPanelings type, containing the panel fineness of the wing for the VSAero model</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" name="wingPaneling" type="aeroFuseWingPanelingType">
						<xs:annotation>
							<xs:documentation>Span- and chordwise paneling of a wing</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseWingTipPanelingType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseWingTipPanelingType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroFuseWingTipPaneling type, containing paneling and secCurv for wing tip</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="0" name="paneling" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Paneling for the wing tip section; if no patch is set for the wing tip, the paneling number can be neglected.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="secCurv">
						<xs:annotation>
							<xs:documentation>Wing tip style. 0 for no wing tip patch, 1 for flat wing tip, 2 (default) for Semicircular sections wing tip</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="integerBaseType">
									<xs:enumeration value="0" />
									<xs:enumeration value="1" />
									<xs:enumeration value="2" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroLandingGearType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>controlSurfacePerformanceMapType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>LandingGearPerformanceMap type, containing a delta perfomance map with aerodynamic data for a landingGear. Array order is: relativeDeflection min-&gt;max then angleOfAttack then angleOfYaw then reynoldsNumber then machNumber. AngleOfAttack, angleOfYaw, reynoldsNumber and machNumber are taken from the basic performance map one level above.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="landingGearUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Reference to the landingGear</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="relDeflection" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Relative deflection of the control surface</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="dcfx" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfy" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfz" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmx" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmy" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmz" type="stringArrayBaseType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroLandingGearsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroLandingGearsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="landingGear" type="aeroLandingGearType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroPerformanceMapRCType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroPerformanceMapType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroPerformanceMap type, containing a perfomance map with aerodynamic data. Array order is: angleOfAttack min-&gt;max then angleOfYaw then reynoldsNumber then machNumber</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="machNumber" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Mach number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reynoldsNumber" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Reynolds Number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="angleOfYaw" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Yaw angle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="angleOfAttack" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Angle of attack</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="analysisTool" type="toolType">
						<xs:annotation>
							<xs:documentation>Name and version of the tool used to compute the aerodynamic performance</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="analysisLevel" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Modeling level of the methods used to compute the aerodynamic performance. The higher the analysisLevel, the higher the quality of the results. Possible use of analysisLevel: 0- 9 = Statistical models, 10-19 = Analytic models, 20-29 = Lifting line method, 30-39 = Panel method, 40-49 = Panel-BL-coupled method, 50-59 = Full potential method, 60-69 = Full potential-BL coupled method, 70-79 = CFD euler method, 80-89 = CFD euler-bl coupled method, 99-99 = CFD RANS method, &gt;=100 = Experimental data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="cfx" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cfy" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cfz" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cmx" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cmy" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cmz" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dampingDerivatives" type="dampingDerivativesRatesType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="aeroPerformanceMapType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroPerformanceMapType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroPerformanceMap type, containing a perfomance map with aerodynamic data. Array order is: angleOfAttack min-&gt;max then angleOfYaw then reynoldsNumber then machNumber.</ns2:para>
						<ns2:para>All coefficients in the aeroperformanceMap relate to the CPACS coordinate system. See documentation of the CPACS-Element for further information.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="machNumber" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Mach number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reynoldsNumber" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Reynolds Number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="angleOfYaw" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Yaw angle</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="angleOfAttack" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>Angle of attack</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="cfx" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cfy" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cfz" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cmx" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cmy" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="cmz" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dampingDerivatives" type="dampingDerivativesRatesType" />
					<xs:element minOccurs="0" name="controlSurfaces" type="controlSurfacePerformanceMapsType" />
					<xs:element maxOccurs="1" minOccurs="0" name="landingGears" type="aeroLandingGearsType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroPerformanceMapsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroPerformanceMapsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroPerformanceMaps type, containing aeroPerformanceMaps</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
