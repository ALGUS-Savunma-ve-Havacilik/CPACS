				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>delisSheetSizingSettingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:annotation>
					<xs:documentation>Settings to define the sizing settings of sheet-like structures. Since stringers may be modeled as smeared into the sheet/shell, stiffeners are also considered.</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="stiffeningType">
						<xs:annotation>
							<xs:documentation>Choice determining the stiffening of the sheet. The names and corresponding geometry is analog to the CPACS &#8220;standardProfileType Element&#8221;.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="T" />
									<xs:enumeration value="Z" />
									<xs:enumeration value="I" />
									<xs:enumeration value="J" />
									<xs:enumeration value="C" />
									<xs:enumeration value="L" />
									<xs:enumeration value="HAT" />
									<xs:enumeration value="unstiffened" />
									<xs:enumeration value="sandwich" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="designCriteria" type="delisDesignCriteriaType" />
					<xs:element maxOccurs="1" minOccurs="0" name="skinMinimalThickness" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Minimal thickness of a sheet(total stacking thickness) Defaults to 0.001</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="delisSizingConvergenceType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>delisSizingConvergenceType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="1" minOccurs="0" name="thicknessThreshold" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Convergence threshold to the maximum thickness change. Defaults to 0.0005 in SBOT</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="maxIterations" type="doubleBaseType">
