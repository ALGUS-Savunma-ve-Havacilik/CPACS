	<xs:complexType name="dampingDerivativesRatesType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Damping derivatives for positive and negative rotation rates</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:section>
								<ns2:title>0. General overview</ns2:title>
								<ns2:content>
									<ns2:para>This type contains the damping derivatives. They are split up into those derivatives for positive rotation rates, and those for negative rotation rates.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>1. &lt;positiveRates&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Damping derivatives, calculated by positive rotation rates.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>2. &lt;negativeRates&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Damping derivatives, calculated by negative rotation rates.</ns2:para>
								</ns2:content>
							</ns2:section>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="0" name="positiveRates" type="dampingDerivativesType" />
					<xs:element minOccurs="0" name="negativeRates" type="dampingDerivativesType" />
				</xs:all>
				<xs:attribute name="uID" type="xsd:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="dampingDerivativesType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Performane maps with damping derivatives</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:section>
								<ns2:title>0. General overview</ns2:title>
								<ns2:content>
									<ns2:para>This type contains aerodynamic performance maps with the damping derivatives. The derivatives are calculated using rotational rates [rad/s], normalized by: Rate*ReferenceLength/flow speed. The rotations are performed around the global axis directions with the aircraft model's global reference point as origin. The damping derivative performance maps are arrays consisting of semicolon separated values, simular to the baseline aerodynamic performance maps.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>1. &lt;dcfxdpstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfx by normalized roll rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>2. &lt;dcfxdqstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfx by normalized pitch rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>3. &lt;dcfxdrstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfx by normalized yaw rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>4. &lt;dcfydpstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfy by normalized roll rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>5. &lt;dcfydqstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfy by normalized pitch rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>6. &lt;dcfydrstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfy by normalized yaw rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>7. &lt;dcfzdpstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfz by normalized roll rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>8. &lt;dcfzdqstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfz by normalized pitch rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>9. &lt;dcfzdrstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cfz by normalized yaw rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>10. &lt;dcmxdpstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmx by normalized roll rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>11. &lt;dcmxdqstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmx by normalized pitch rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>12. &lt;dcmxdrstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmx by normalized yaw rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>13. &lt;dcmydpstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmy by normalized roll rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>14. &lt;dcmydqstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmy by normalized pitch rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>15. &lt;dcmydrstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmy by normalized yaw rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>16. &lt;dcmzdpstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmz by normalized roll rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>17. &lt;dcmzdqstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmz by normalized pitch rate</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>18. &lt;dcmzdrstar&gt; (optional)</ns2:title>
								<ns2:content>
									<ns2:para>Change of cmz by normalized yaw rate</ns2:para>
								</ns2:content>
							</ns2:section>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="0" name="dcfxdpstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfxdqstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfxdrstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfydpstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfydqstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfydrstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfzdpstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfzdqstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcfzdrstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmxdpstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmxdqstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmxdrstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmydpstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmydqstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmydrstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmzdpstar" type="stringArrayBaseType" />
					<xs:element minOccurs="0" name="dcmzdqstar" type="stringArrayBaseType" />
