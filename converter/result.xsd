			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="toComponentSegmentUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>UID of the componentSegment, that is connected with the current one.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="elements" type="wingWingAttachmentElementsType" />
					<xs:element name="sparAttachments" type="wingWingAttachmentsSparsType" />
					<xs:element name="upperShellAttachment" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Defines if the upper shell of the current componentSegment is structurally connected to the upper or lower shell of the second componentSegment. Can have the values 'upperShell' or 'lowerShell'.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lowerShellAttachment" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Defines if the lower shell of the current componentSegment is structurally connected to the upper or lower shell of the second componentSegment. Can have the values 'upperShell' or 'lowerShell'.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


