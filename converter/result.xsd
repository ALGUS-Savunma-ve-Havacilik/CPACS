	<xs:complexType name="routeType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Describes a specific route</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Describes a specific route, e.g. FRA-EWR, which is operated by a speicifc air vehicle (modelUID) on a specific mission profile (missionUID). The route is further described by the UTC starting time.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Description of the route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="modelUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>The aircraft operated on this route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="missionUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>The mission specified for this route</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="utcStartingTime" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Starting time of the flight expressed as Greenwich (Zulu) time in UTC format: JJJJ:MM:DDThh:mm:ss, e.g. 2011:06:16T12:31:51</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="frequency" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>The frequency at which this flight is operated in flights per year</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="paxLoadFactor" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>The specific pax load factor for this route, ranges from 0...1</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="cargoLoadFactor" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>The specific cargo load factor for this route, ranges from 0...1</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="analysis" type="routeAnalysisType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


	<xs:complexType name="routesType">
		<!--****************************************************
--><xs:annotation>
