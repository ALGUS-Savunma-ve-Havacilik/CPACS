	<xs:complexType name="deckType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>deckType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Deck type, containing the data of a deck</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of the deck</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Description of the deck</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="parentUID" type="stringUIDBaseType" />
					<xs:element name="x0" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>The starting point of the deck/cabin. In a conventional aircraft like the A320, it would be the rear wall of the cockpit. The coordinate is relative to the parent object defined by &#8220;parentUID&#8221;, which should be the fuselage.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="z0" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>The starting point of the deck/cabin. In a conventional aircraft like the A320, it would be the rear wall of the cockpit. The coordinate is relative to the parent object defined by &#8220;parentUID&#8221;, which should be the fuselage.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="type">
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="passenger" />
									<xs:enumeration value="VIP" />
									<xs:enumeration value="cargo" />
									<xs:enumeration value="lifeStock" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="cabGeometry" type="cabGeometryType" />
					<xs:element maxOccurs="1" minOccurs="0" name="aisles" type="cabinAislesType" />
					<xs:element maxOccurs="1" minOccurs="0" name="spaces" type="cabinSpacesType" />
					<xs:element maxOccurs="1" minOccurs="0" name="doors" type="cabinDoorsType" />
					<xs:element maxOccurs="1" minOccurs="0" name="seatElements" type="cabinSeatElementsType" />
					<xs:element maxOccurs="1" minOccurs="0" name="floorElements" type="cabinFloorElementsType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


