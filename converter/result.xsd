			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="0" name="wingPanelings" type="aeroFuseWingPanelingsType">
						<xs:annotation>
							<xs:documentation>Paneling for each wing</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="fuselagePanelings" type="aeroFuseFuselagePanelingsType">
