	<xs:complexType name="flightLoadCaseType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>flightLoadCaseType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:section>
								<ns2:title>Main class</ns2:title>
								<ns2:content>
									<ns2:para>
										<ns2:legacyBold>State</ns2:legacyBold>
									</ns2:para>
									<ns2:para>Includes general definition of aircraft attitude and the aircraft environment.</ns2:para>
									<ns2:para>
										<ns2:legacyBold>Aircraft Settings</ns2:legacyBold>
									</ns2:para>
									<ns2:para>Include definition of settings of the aircraft self. For example setting of a flap or landinggear.</ns2:para>
									<ns2:para>
										<ns2:legacyBold>MassCase</ns2:legacyBold>
									</ns2:para>
									<ns2:para>Include definition about the massCase and is linked by an UID to the massBreakdown, weightAndBalance or direct component link. ?!</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>References</ns2:title>
								<ns2:content>
									<ns2:para>
										<ns2:legacyBold>Coordinate System(s)</ns2:legacyBold>
										<ns2:list class="bullet">
											<ns2:listItem>Earth Fix coord.sys. (EF) with planar surface (x-y-Ebene = Earth surface, x North, y East)</ns2:listItem>
										</ns2:list>
										<ns2:para>
											<ns2:mediaLink>
												<ns2:image ns3:href="earthFixCoordSys" />
											</ns2:mediaLink>
										</ns2:para>
										<ns2:list class="bullet">
											<ns2:listItem>refCoordUid (e.g. body fix and orientation to EF)</ns2:listItem>
											<ns2:listItem>refpointUid</ns2:listItem>
										</ns2:list>
										<ns2:para>
											<ns2:mediaLink>
												<ns2:image ns3:href="bodyFixCoordSys2" />
											</ns2:mediaLink>
										</ns2:para>
									</ns2:para>
									<ns2:para>
										<ns2:legacyBold>Euler angles</ns2:legacyBold>
										<ns2:list class="bullet">
											<ns2:listItem>Attitude of body fix to earth fix coord.sys.</ns2:listItem>
											<ns2:listItem>&#968; psi &#8211; heading</ns2:listItem>
											<ns2:listItem>&#952; theta &#8211; pitch angle</ns2:listItem>
											<ns2:listItem>&#934; phi &#8211; bank angle</ns2:listItem>
											<ns2:listItem>Rotation order: &#968;, &#952;, &#934;</ns2:listItem>
										</ns2:list>
									</ns2:para>
									<ns2:para>
										<ns2:mediaLink>
											<ns2:image ns3:href="eulerAnglesInCoordSys" />
										</ns2:mediaLink>
									</ns2:para>
									<ns2:para>
										<ns2:legacyBold>Reference of attitude values</ns2:legacyBold>
										<ns2:list class="bullet">
											<ns2:listItem>x,y,z (vector to ref point in EF)</ns2:listItem>
											<ns2:listItem>dx,dy,dz (vector in EF)</ns2:listItem>
											<ns2:listItem>u,v,w (vector in refCoord) (body fix)</ns2:listItem>
											<ns2:listItem>du,dv,dw (vector in refCoordUid) (body fix)</ns2:listItem>
										</ns2:list>
									</ns2:para>
									<ns2:para>
										<ns2:mediaLink>
											<ns2:image ns3:href="bodyFixCoordSys" />
										</ns2:mediaLink>
									</ns2:para>
									<ns2:para>(dx,dy,dy --&gt; Euler transformation (psi, theta, phi) --&gt; u,v,w)</ns2:para>
								</ns2:content>
							</ns2:section>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Load case name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Load case description (String)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="aeroDataSetUID" type="stringUIDBaseType" />
					<xs:element maxOccurs="1" minOccurs="0" name="loadCondition" type="loadConditionType" />
					<xs:element name="state" type="loadCaseStateType" />
					<xs:element maxOccurs="1" minOccurs="0" name="aircraftSettings" type="loadCaseSettingsType" />
					<xs:element maxOccurs="1" minOccurs="0" name="mass" type="loadCaseMassType" />
					<xs:element maxOccurs="1" minOccurs="0" name="nodalLoads" type="nodalLoadsType" />
					<xs:element maxOccurs="1" minOccurs="0" name="cutLoads" type="cutLoadsType" />
					<xs:element maxOccurs="1" minOccurs="0" name="externalAmifFile" type="stringBaseType" />
