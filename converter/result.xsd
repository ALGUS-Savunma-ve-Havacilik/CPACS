									<xs:enumeration value="EDDL" />
									<xs:enumeration value="EDDF" />
									<xs:enumeration value="EDDT" />
									<xs:enumeration value="ESSA" />
									<xs:enumeration value="EDVE" />
									<xs:enumeration value="EGSS" />
									<xs:enumeration value="EGLL" />
