	<xs:complexType name="hostTempType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>hostTempType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The hostTempType is used to temporarily store HOST inputs and outputs.</ns2:para>
					</ns2:remarks>
