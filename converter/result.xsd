				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroPerformanceType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroPerformance type, containing perfomance maps with aerodynamic data of an airfoil.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="0" name="globalAeroPerformance" type="globalAeroPerformanceType">
						<xs:annotation>
							<xs:documentation>Aerodynamic performance map of the full configuration</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="fuselagesAeroPerformance" type="fuselagesAeroPerformanceType">
						<xs:annotation>
							<xs:documentation>Aerodynamic performance maps of isolated fuselages</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="wingsAeroPerformance" type="wingsAeroPerformanceType">
