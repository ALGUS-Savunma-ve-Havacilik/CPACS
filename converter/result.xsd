	<xs:complexType name="aeroFuseWakePanelingType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseWakePanelingType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroFuseWakePanelingType, contain the fineness of the wake panels for one of the streamwise wake segments</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="xPaneling" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Streamwise paneling of wake</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="xMax" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Location of the farthest x plane of the current wake segment. Recommended for the last wake plane: 10*Span or 100*Chord</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="interpolate">
						<xs:annotation>
							<xs:documentation>Form of interval spacing for the generated panels. 0=Full cosine spacing, 1=Half-cosine with smaller spacing near the preceding node, 2=Half-cosine with smaller spacing near this node (default), 3=Equal spacing</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="integerBaseType">
									<xs:enumeration value="0" />
									<xs:enumeration value="1" />
									<xs:enumeration value="2" />
									<xs:enumeration value="3" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseWakePanelingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseWakePanelingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroFuseWakePanelings type, containing the fineness of the wake panels for the VSAero model</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" name="wakePaneling" type="aeroFuseWakePanelingType">
						<xs:annotation>
							<xs:documentation>Streamwise paneling of a wake segment. If there is more than one wake plane, the x coordinate of each wake plane must be greater than the x coordinate of the preceding wake plane.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseWingPanelingType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseWingPanelingType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroFuseWingPaneling type, containing the span- and chordwise paneling for the wings of the VSAero model</ns2:para>
