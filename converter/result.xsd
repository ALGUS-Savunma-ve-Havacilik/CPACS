			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="tool" type="toolType">
						<xs:annotation>
							<xs:documentation>Tool identification</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="aircraftModelUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>UID of the aircraft model</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="cFDToolType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>cFDToolType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
