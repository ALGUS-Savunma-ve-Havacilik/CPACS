					<xs:element name="airfoilUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Reference to the airfoil uID.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="rotX" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Rotation around an axis, going from the leading edge point to the trailing edge point of the inner/outer border of the control surface. Defaults to 90&#176;, which is equivalent to perpendicular on the control surface middle plane.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="scalY" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Scaling of the airfoil in spanwise direction (not used for 2D airfoils).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="scalZ" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Scaling in thickness direction of the airfoil.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
