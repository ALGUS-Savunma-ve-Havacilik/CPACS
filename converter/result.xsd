	<xs:complexType name="aeroFuseSurfacePanelingsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseSurfacePanelingsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroFuseSurfacePanelings type, containing the fineness of the surface panels for the VSAero model</ns2:para>
