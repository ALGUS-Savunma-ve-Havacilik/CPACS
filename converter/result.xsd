	<xs:complexType name="delisDesignCriteriaType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>delisDesignCriteriaType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:annotation>
					<xs:documentation>List of design criteria used. Design criteria that are not listed, will not be used. Defaults to [stressBasedFailure, localBuckling, globalBuckling, crippling]</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="designCriterion">
						<xs:annotation>
							<xs:documentation>Activates the given design criterion.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="strainBasedFailure" />
									<xs:enumeration value="stressBasedFailure" />
									<xs:enumeration value="localBuckling" />
									<xs:enumeration value="globalBuckling" />
									<xs:enumeration value="crippling" />
									<xs:enumeration value="fatique" />
									<xs:enumeration value="damageTolerance" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
