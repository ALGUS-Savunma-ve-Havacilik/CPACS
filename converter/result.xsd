							<xs:documentation>Destination region of the flight</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="D" />
									<xs:enumeration value="UK" />
									<xs:enumeration value="EU" />
									<xs:enumeration value="EUR" />
									<xs:enumeration value="World" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
