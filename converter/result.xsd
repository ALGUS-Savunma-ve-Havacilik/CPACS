	<xs:complexType name="aeroPoleAirfoilAeroPerformanceType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroPoleAirfoilAeroPerformanceType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroPoleAirfoilAeroPerformanceType type, containing options for the genaration of the aerodynamic performance maps for the airfoil</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="1" name="mode" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Mode for the generation of aero performance data. Possible values: "0" - Do not add any performace data (default); "1" - Add performance data to the CPACS dataset; "2" - Return HOST airfoil polar files (in the ZIP file).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="destinationModelUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Destination UID of the aircraft/rotorcraft model where the airfoil performance analysis should be inserted. Defaults to the first model found in the CPACS dataset.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroPoleAirfoilGeometryOutputOptionsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroPoleAirfoilGeometryOutputOptionsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>aeroPoleAirfoilGeometryOutputOptionsType type, containing output options for the airfoil geometry</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element minOccurs="1" name="destinationAirfoilUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Destination UID which the created airfoil should get. If an airfoil with the specified UID does already exist, it will be overwritten.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="destinationProfileCatalog">
						<xs:annotation>
							<xs:documentation>This option specifies into which profile catalog node the airfoil geometry is inserted. Possible values: - "wingAirfoils" - "rotorAirfoils" (default) - "fuselageProfiles"</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="wingAirfoils" />
									<xs:enumeration value="rotorAirfoils" />
									<xs:enumeration value="fuselageProfiles" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element minOccurs="0" name="coordinateMapping">
						<xs:annotation>
							<xs:documentation>This option specifies to which axes the original xy-coordinates of the catalog airfoil should be mapped. Possible values: - "xy" - "xz" (default) - "yz"</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="xy" />
									<xs:enumeration value="xz" />
									<xs:enumeration value="yz" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

