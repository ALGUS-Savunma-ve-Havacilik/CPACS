	<xs:complexType name="controlSurfaceWingCutOutType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Cut out of the parents structure due to a control surface.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:para>Optional. Definition of the parents structure cut out due to a control surface. The cut out is split into three parts: cut out of the upper and lower skin and the definition of an profile conecting the cut out of the upper and lower skin.</ns2:para>
							<ns2:para>An example for wing cut outs can be found in the picture below:</ns2:para>
							<ns2:mediaLink>
								<ns2:image ns3:href="wingCutOut" />
							</ns2:mediaLink>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="upperSkin" type="controlSurfaceSkinCutOutType" />
					<xs:element name="lowerSkin" type="controlSurfaceSkinCutOutType" />
					<xs:element minOccurs="0" name="cutOutProfileControlPoint" type="cutOutControlPointsType" />
					<xs:element minOccurs="0" name="cutOutProfiles" type="cutOutProfilesType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


