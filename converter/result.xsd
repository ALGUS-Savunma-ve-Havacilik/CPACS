	<xs:complexType name="comradeVariableType">
		<!--****************************************************
--><xsd:annotation><xsd:appinfo><schemaDoc><ddue:summary><ddue:para>comradeVariableType</ddue:para></ddue:summary><ddue:remarks><ddue:para /></ddue:remarks></schemaDoc></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="complexBaseType"><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>comradeVariableType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The comradeVariableType stores a name-value pair of a COMRADE variable.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="stringBaseType">
				<xs:attribute name="name" type="xsd:string" use="required">
					<xs:annotation>
