					<xs:element name="piston" type="pistonType" />
					<xs:element name="dragStrut" type="strutWithActuatorType" />
					<xs:element name="pintleStrut" type="strutType" />
					<xs:element name="sideStruts" type="sideStrutsType" />
					<xs:element name="mainActuator" type="mainActuatorType" />
					<xs:element minOccurs="0" name="bogie" type="bogieType" />
					<xs:element name="axles" type="axlesType" />
