						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Description of the rib set.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="ribCrossSection" type="wingRibCrossSectionType" />
					<xs:element minOccurs="0" name="ribsPositioning" type="pylonRibsPositioningType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" />
			</xs:extension>
