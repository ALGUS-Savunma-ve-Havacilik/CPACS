--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>systemsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:para>Systems type, containing the aircraft's control system data</ns2:para>
							<ns2:para>Please see the attached picture for further documentation</ns2:para>
							<ns2:mediaLink>
								<ns2:image ns3:href="controls" />
							</ns2:mediaLink>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="cockpitControls" type="cockpitControlsType">
						<xs:annotation>
							<xs:documentation>Cockpit controls, e.g. stickRoll, pedals</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="commandCases" type="commandCasesType">
						<xs:annotation>
							<xs:documentation>Different commandCases that are commanded, e.g. roll, accelerate</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="controlDistributors" type="controlDistributorsType">
						<xs:annotation>
							<xs:documentation>Control Distributors, deliver inputs to the control actuators. E.g. different angles of different ailerons.</xs:documentation>
						</xs:annotation>
					</xs:element>
