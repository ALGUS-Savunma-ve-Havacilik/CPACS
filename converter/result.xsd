	<xs:complexType name="aeroFuseToolParametersType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseToolParametersType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroFuseToolParameters type, containing the parameters for VSAero tool control</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="archiveMode">
						<xs:annotation>
							<xs:documentation>Archive mode for results. 0=nothing, 1=inputfiles, 2=all</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="integerBaseType">
									<xs:enumeration value="0" />
									<xs:enumeration value="1" />
									<xs:enumeration value="2" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="panelings" type="aeroFusePanelingsType">
						<xs:annotation>
							<xs:documentation>Panel fineness for VSAero model</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="trefftzComputation">
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="cfx" />
									<xs:enumeration value="cfz" />
									<xs:enumeration value="cfxcfz" />
									<xs:enumeration value="none" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="wakeComputation" type="aeroFuseWakeComputationType" />
					<xs:element maxOccurs="1" minOccurs="0" name="viscousComputation" type="aeroFuseViscousComputationType" />
					<xs:element maxOccurs="1" minOccurs="0" name="compressibilityCorrection">
						<xs:annotation>
							<xs:documentation>Compressibility correction mode. Possible values are: "off" (default), "Lieblein-Stockmann", "Karman-Tsien" and "Prandtl-Glauert".</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="off">
										<xs:annotation>
											<xs:documentation>Do not apply any compressibility correction</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="Lieblein-Stockman">
										<xs:annotation>
											<xs:documentation>Lieblein-Stockman compressibility correction</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="Karman-Tsien">
										<xs:annotation>
											<xs:documentation>Karman-Tsien compressibility correction</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
									<xs:enumeration value="Prandtl-Glauert">
										<xs:annotation>
											<xs:documentation>Prandtl-Glauert compressibility correction with flux boundary conditions and second-order pressure coefficient</xs:documentation>
										</xs:annotation>
									</xs:enumeration>
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseToolType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseToolType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>AeroFuseTool type, containing VSAero tool data</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="tool" type="toolType">
						<xs:annotation>
							<xs:documentation>Tool identification</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="modelUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>Reference to aircraft or rotorcraft model</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="datasetName" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of the dataset for VSAero calculation</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="referenceValues" type="vSAeroReferenceType">
						<xs:annotation>
							<xs:documentation>Reference values for the calculation</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="operatingConditions" type="aeroFuseOperatingConditionType">
						<xs:annotation>
							<xs:documentation>Definition of operating conditions</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="toolParameters" type="aeroFuseToolParametersType">
						<xs:annotation>
							<xs:documentation>Parameters for tool control</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseViscousComputationType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseViscousComputationType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="streamlines">
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="auto" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element name="iterations" type="integerBaseType" />
					<xs:element maxOccurs="1" minOccurs="0" name="separationEstimation">
						<xs:annotation>
							<xs:documentation>Add estimated forces and moments due to areas of separated flow to the total force and moment coefficients. Possible values are "off" (default) or "on".</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="on" />
									<xs:enumeration value="off" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="boundaryLayerTranspiration">
						<xs:annotation>
							<xs:documentation>Include the effect of boundary layer transpiration in the viscous calculation. Possible values are "on" (default, viscous effect includes boundary layer transpiration and skin friction) or "off" (viscous effect is skin friction only)</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="on" />
									<xs:enumeration value="off" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="aeroFuseWakeComputationType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>aeroFuseWakeComputationType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
