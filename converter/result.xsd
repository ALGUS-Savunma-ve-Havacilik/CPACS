				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>genericGeometryComponentsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="genericGeometryComponent" type="genericGeometricComponentType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="genericMassType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>mass</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>genericMassType, describing mass, inertia and CoG data.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>description</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>parentUID</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="mass" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>mass value</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="location" type="pointType">
						<xs:annotation>
							<xs:documentation>mass loaction values (x,y,z)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="orientation" type="pointType">
						<xs:annotation>
							<xs:documentation>mass orientation values</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="massInertia" type="massInertiaType" />
				</xs:sequence>
				<xs:attribute name="uID" type="xsd:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenAssemblyType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenAssemblyType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenAssemblyType contains all informations required by GeoGen to assemble fuselages, wings and rotors in CATIA and to output the resulting geometries into the CPACS dataset.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="0" name="reference" type="geogenReferenceType">
						<xs:annotation>
							<xs:documentation>Global reference lengths and point used by GeoGen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="constraints" type="geogenConstraintsType">
						<xs:annotation>
							<xs:documentation>Geometry constraints to be checked</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="fuselages" type="geogenFuselagesType">
						<xs:annotation>
							<xs:documentation>Definition of fuselage assemblies</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="rotorBlades" type="geogenRotorBladesType">
						<xs:annotation>
							<xs:documentation>Definition of rotorBlade assemblies</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="wings" type="geogenWingsType">
						<xs:annotation>
							<xs:documentation>Definition of wing assemblies</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenAttachedMassPositionType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenAttachedMassPositionType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenAttachedMassPositionType is used to define a component mass position using a CATIA UDF from the GeoGen Catalog, which outputs a point in the CATIA modrl built by GeoGen. GeoGen will output the global coordinates of each geogenAttachedMassPosition to the XPath (locationGlobalDestinationXPath) given by the user.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="0" name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of the attached mass.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Description of the attached mass.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="catalogPointComponentUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the attachment component, as defined in the component catalog.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="parameters" type="geogenParametersType">
						<xs:annotation>
							<xs:documentation>Parameter definitions for the attachment component. The possible parameter definitions depend on the component definition in the component catalog.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="componentReferences" type="geogenComponentReferencesType">
						<xs:annotation>
							<xs:documentation>References to components which were created by geogen previously.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="destinationXPath" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>XPath of the massDescription to which the calculated global coordinates should be written. If omitted, the mass position will not be written to the output CPACS file.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
				<xs:attribute name="uID" type="xsd:string">
					<xs:annotation>
						<xs:documentation>Unique ID of the attachmentMassPosition.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenAttachedMassPositionsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenAttachedMassPositionsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenAttachedMassPositionsType contains a collection of GeoGen attachedMassPosition definitions.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="attachedMassPosition" type="geogenAttachedMassPositionType">
						<xs:annotation>
							<xs:documentation>GeoGen attachedMassPositions definition</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenComponentReferenceType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenComponentReferenceType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenComponentReferenceType can be used to refer to an existing component instead of creating a new component.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="0" name="catalogSubcomponentUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the subcomponent as defined in the component catalog</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
				<xs:attribute name="refUID" type="xsd:string" use="required">
					<xs:annotation>
						<xs:documentation>UID of the existing component to which the reference should point. This component must be a child element of the same</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenComponentReferencesType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenComponentReferencesType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenComponentReferencesType contains a collection of references to existing components</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="componentReference" type="geogenComponentReferenceType">
						<xs:annotation>
							<xs:documentation>Include one &lt;componentReference&gt; element for each component reference definition</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenComponentType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenComponentType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenComponentType contains all information needed by GeoGen to create an instance of a component from the component catalog</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="name" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Name of the component</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="description" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Optional description of the component</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="catalogSubcomponentUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the subcomponent as defined in the component catalog. Required if the component is a subcomponent of another component.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="catalogComponentUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the component as defined in the component catalog</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="parameters" type="geogenParametersType">
						<xs:annotation>
							<xs:documentation>Parameter definitions for the component. The possible parameter definitions depend on the component definition in the component catalog.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="components" type="geogenComponentsType">
						<xs:annotation>
							<xs:documentation>Subcomponent definitions for the component. The possible subcomponent definitions depend on the component definition in the component catalog.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="componentReferences" type="geogenComponentReferencesType">
						<xs:annotation>
							<xs:documentation>References to components which where defined elsewhere but should be used as subcomponents</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
				<xs:attribute name="uID" type="xsd:string">
					<xs:annotation>
						<xs:documentation>Unique ID of the component. Can be used to refer to this component using a &lt;componentReference&gt; element</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenComponentsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenComponentsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenComponentsType contains a collection of (sub-)components</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="component" type="geogenComponentType">
						<xs:annotation>
							<xs:documentation>Include one &lt;component&gt; element for each (sub-)component definition</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenConstraintOffsetType">
		<!--****************************************************
--><xsd:annotation><xsd:appinfo><schemaDoc><ddue:summary><ddue:para>geogenConstraintOffsetType</ddue:para></ddue:summary><ddue:remarks><ddue:para /></ddue:remarks></schemaDoc></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="complexBaseType"><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenConstraintOffsetType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenConstraintOffsetType defines the offset distance for offset surfaces used as constraints for GeoGen.</ns2:para>
						<ns2:para>For inner boundaries the offset will be added to the original surface, while for outer boundaries the offset will be subtracted from the original surface.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="stringBaseType">
				<xs:attribute name="mode" type="xsd:string">
					<xs:annotation>
						<xs:documentation>Mode for calculation of the offset surface: 1 = Normal to the surface, 2 = z-Direction</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xsd:extension></xsd:complexContent></xs:complexType>

	<xs:complexType name="geogenConstraintType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenConstraintType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenConstraintType can be used to define inner or outer boundary surfaces which shall not be penetrated by the resulting surface geometries.</ns2:para>
						<ns2:para>Constraints behave differently for inner and outer boundaries:</ns2:para>
						<ns2:list class="bullet">
							<ns2:listItem>
								<ns2:para>
									<ns2:legacyBold>Inner boundaries:</ns2:legacyBold> The volume enclosed by the referenced fuselage surface and thickened by the optional offset has to be within the volume enclosed by the created fuselages and must not penetrate it's boundaries.</ns2:para>
							</ns2:listItem>
							<ns2:listItem>
								<ns2:para>
									<ns2:legacyBold>Outer boundaries:</ns2:legacyBold> The volume enclosed by the referenced fuselage surface minus the optional offset must contain the volume enclosed by the created fuselages. The created fuselage surface must not penetrate it's boundaries.</ns2:para>
							</ns2:listItem>
						</ns2:list>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="fuselageUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the fuselage which should be used as boundary. This fuselage definition must exist in the CPACS dataset before GeoGen is run and it must be a valid geometry.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="offset" type="geogenConstraintOffsetType">
						<xs:annotation>
							<xs:documentation>Optional offset which is added to (for inner boundaries) or subtracted from (for outer boundaries) the referenced fuselage's surface to create the final boundary surface.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenConstraintsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenConstraintsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Definition of the inner and outer boundaries which should not be penetrated by the resulting geometries.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="innerBounds" type="geogenConstraintType">
						<xs:annotation>
							<xs:documentation>Inner boundary for the resulting geometries</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="outerBounds" type="geogenConstraintType">
						<xs:annotation>
							<xs:documentation>Outer boundary for the resulting geometries</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenFuselageOutputOptionsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenFuselageOutputOptionsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenFuselageOutputOptionsType contains options used during the export of a fuselage from CATIA to CPACS.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="destinationFuselageUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Destination UID of the fuselage which will be written into the CPACS file.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="sectionDistributionMode">
						<xs:annotation>
							<xs:documentation>Section distribution mode. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="automatic" />
									<xs:enumeration value="distributionLawEquidistant" />
									<xs:enumeration value="distributionLawFullCosine" />
									<xs:enumeration value="distributionLawHalfCosineFront" />
									<xs:enumeration value="distributionLawHalfCosineRear" />
									<xs:enumeration value="manual" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of sections. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of points per section. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenFuselageType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenFuselageType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenFuselageType contains all information needed by GeoGen to assemble component templates as a fuselage and to export the geometry to CPACS.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="0" name="reference" type="geogenReferenceType">
						<xs:annotation>
							<xs:documentation>Reference lengths and point used for this fuselage (optional). If not specified, the global reference lengths and point are used.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="components" type="geogenComponentsType">
						<xs:annotation>
							<xs:documentation>Collection of all component templates to be assembled as a fuselage. Allowed are definitions of components which are valid subcomponents of the componentType 'Fuselage', as specified in the used component catalog. Refer to the documentation of the used component catalog for further information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="parameters" type="geogenParametersType">
						<xs:annotation>
							<xs:documentation>Optional parameters for the componentType 'Fuselage', as specified in the used component catalog. Refer to the documentation of the used component catalog for further information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="outputOptions" type="geogenFuselageOutputOptionsType">
						<xs:annotation>
							<xs:documentation>Output options for this fuselage.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenFuselagesType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenFuselagesType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Elements of type geogenFuselagesType contain definitions of fuselage assemblies which should be generated by GeoGen.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="fuselage" type="geogenFuselageType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenOutputOptionsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenOutputOptionsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenOutputOptionsType contains all global output options for GeoGen.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="destinationModelUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the aircraft or rotorcraft model to which the generated fuselages/wings/rotors should be added</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Default number of sections for all generated fuselages/wings/rotorblades</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Default number of points per section for all generated fuselages/wings/rotorblades</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenParameterConstraintType">
		<!--****************************************************
--><xsd:annotation><xsd:appinfo><schemaDoc><ddue:summary><ddue:para>geogenParameterConstraintType</ddue:para></ddue:summary><ddue:remarks><ddue:para /></ddue:remarks></schemaDoc></xsd:appinfo></xsd:annotation><xsd:complexContent><xsd:extension base="complexBaseType"><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenParameterConstraintType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenParameterConstraintType is used to set upper and lower bounds for GeoGen parameters.</ns2:para>
						<ns2:para>The value can be any valid string for the parameter type defined in the component catalog.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="stringBaseType">
				<xs:attribute default="1" name="inclusive" type="xsd:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Indicates whether the the boundary is included in the valid range of the parameter ('&lt;=', '&gt;=') or not ('&lt;', '&gt;').</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xsd:extension></xsd:complexContent></xs:complexType>

	<xs:complexType name="geogenParameterType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenParameterType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenParameterType contains the definition of a GeoGen parameter.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="catalogParameterUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>UID of the parameter as defined in the component catalog used by GeoGen.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="type" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Parameter type can be 'fixed'(parameter is set to a fixed value) or 'free'(parameter value is set by GeoGen during the internal optimization process)</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="value" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Actual value of the parameter</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="min" type="geogenParameterConstraintType">
						<xs:annotation>
							<xs:documentation>Lower boundary value for the parameter. Only used during the internal optimization of parameters of type 'free'</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="max" type="geogenParameterConstraintType">
						<xs:annotation>
							<xs:documentation>Upper boundary value for the parameter. Only used during the internal optimization of parameters of type 'free'</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="initialValue" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Initial value of the parameter. Only used during the internal optimization of parameters of type 'free'</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="step" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Initial step for the optimization of the parameter. Only used for parameters of type 'free'</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
				<xs:attribute name="uID" type="xsd:string" use="optional">
					<xs:annotation>
						<xs:documentation>An optional unique ID that can be used as a reference to the parameter</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenParametersType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenParametersType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenParametersType contains a collection of GeoGen parameter definitions.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="parameter" type="geogenParameterType">
						<xs:annotation>
							<xs:documentation>GeoGen parameter definition</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenReferenceType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenReferenceType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenReferenceType is used to define the reference lengths and the reference point used by Geogen.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="length" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Reference length in [m] used to dimensionalize all parameters of type 'length', which do not include a unit in their &lt;value&gt; element. GeoGen uses the default value 1 [m] if the element is not specified or if it contains invalid data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="width" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Reference length in [m] used to dimensionalize all parameters of type 'width', which do not include a unit in their &lt;value&gt; element. GeoGen uses the default value 1 [m] if the element is not specified or if it contains invalid data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="height" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Reference length in [m] used to dimensionalize all parameters of type 'height', which do not include a unit in their &lt;value&gt; element. GeoGen uses the default value 1 [m] if the element is not specified or if it contains invalid data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="area" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Reference area in [m2] used to dimensionalize all parameters of type 'area', which do not include a unit in their &lt;value&gt; element. GeoGen uses the default value 1 [m2] if the element is not specified or if it contains invalid data.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="point" type="pointType">
						<xs:annotation>
							<xs:documentation>Reference point used by GeoGen to position the component assemblies in the global coordinate system. GeoGen uses the default (0; 0; 0) if the element is not specified or if it contains invalid data.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenRotorBladeOutputOptionsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenRotorBladeOutputOptionsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenRotorBladeOutputOptionsType contains options used during the export of a rotorBlade from CATIA to CPACS.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="destinationRotorBladeUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Destination UID of the rotorBlade which will be written into the CPACS file.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="destinationRotorUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Destination UID of the rotor to which the rotorBlade will be attached.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="sectionDistributionMode">
						<xs:annotation>
							<xs:documentation>Section distribution mode. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="automatic" />
									<xs:enumeration value="distributionLawEquidistant" />
									<xs:enumeration value="distributionLawFullCosine" />
									<xs:enumeration value="distributionLawHalfCosineFront" />
									<xs:enumeration value="distributionLawHalfCosineRear" />
									<xs:enumeration value="manual" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of sections. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of points per section. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenRotorBladeType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenRotorBladeType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenRotorBladeType contains all information needed by GeoGen to assemble component templates as a rotorBlade and to export the geometry to CPACS.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="0" name="reference" type="geogenReferenceType">
						<xs:annotation>
							<xs:documentation>Reference lengths and point used for this rotorBlade (optional). If not specified, the global reference lengths and point are used.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="transformation" type="transformationType" />
					<xs:element maxOccurs="1" minOccurs="1" name="components" type="geogenComponentsType">
						<xs:annotation>
							<xs:documentation>Collection of all component templates to be assembled as a rotorBlade. Allowed are definitions of components which are valid subcomponents of the componentType 'RotorBlade', as specified in the used component catalog. Refer to the documentation of the used component catalog for further information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="parameters" type="geogenParametersType">
						<xs:annotation>
							<xs:documentation>Optional parameters for the componentType 'RotorBlade', as specified in the used component catalog. Refer to the documentation of the used component catalog for further information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="outputOptions" type="geogenRotorBladeOutputOptionsType">
						<xs:annotation>
							<xs:documentation>Output options for this rotorBlade.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenRotorBladesType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenRotorBladesType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>Elements of type geogenRotorBladesType contain definitions of rotorBlades which should be generated by GeoGen.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="rotorBlade" type="geogenRotorBladeType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenToolType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenToolType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenToolType contains all toolspecific input data required by the tool GeoGen.</ns2:para>
						<ns2:para>GeoGen is a geometry generation tool based on the commertial CAD application CATIA V5. It generates fuselage, wing and rotor definitions by assembling predifined fully parametric component templates in CATIA and exporting the resulting geometries into CPACS.</ns2:para>
						<ns2:para>GeoGen is being developed at the DLR Institute for Aerodynamics and Flow Technology (AS) in terms of the project RIDE (Rotorcraft Integrated Design and Evaluation).</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="tool" type="toolType">
						<xs:annotation>
							<xs:documentation>Tool identification</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="assembly" type="geogenAssemblyType">
						<xs:annotation>
							<xs:documentation>Definition of the fuselage, wing and rotor components to be created by GeoGen</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="attachedMassPositions" type="geogenAttachedMassPositionsType">
						<xs:annotation>
							<xs:documentation>Definition of mass positions using GeoGen point components.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="1" name="outputOptions" type="geogenOutputOptionsType">
						<xs:annotation>
							<xs:documentation>Global output options for GeoGen</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenWingOutputOptionsType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenWingOutputOptionsType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para>The geogenWingOutputOptionsType contains options used during the export of a wing from CATIA to CPACS.</ns2:para>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element maxOccurs="1" minOccurs="1" name="destinationWingUID" type="stringBaseType">
						<xs:annotation>
							<xs:documentation>Destination UID of the wing which will be written into the CPACS file.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="parentUID" type="stringUIDBaseType">
						<xs:annotation>
							<xs:documentation>UID of part to which the wing is mounted (if any). The parent of the wing can e.g. be the fuselage. In each aircraft model, there is exactly one part without a parent part (The root of the connection hierarchy).</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="symmetry">
						<xs:annotation>
							<xs:documentation>If there exists a mirrored counterpart of the wing: specify the symmetry plane here</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="x-y-plane" />
									<xs:enumeration value="x-z-plane" />
									<xs:enumeration value="y-z-plane" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="sectionDistributionMode">
						<xs:annotation>
							<xs:documentation>Section distribution mode. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:simpleContent>
								<xs:restriction base="stringBaseType">
									<xs:enumeration value="automatic" />
									<xs:enumeration value="distributionLawEquidistant" />
									<xs:enumeration value="distributionLawFullCosine" />
									<xs:enumeration value="distributionLawHalfCosineFront" />
									<xs:enumeration value="distributionLawHalfCosineRear" />
									<xs:enumeration value="manual" />
								</xs:restriction>
							</xs:simpleContent>
						</xs:complexType>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfSections" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of sections. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element maxOccurs="1" minOccurs="0" name="numberOfPointsPerSection" type="integerBaseType">
						<xs:annotation>
							<xs:documentation>Number of points per section. If not specified, the value specified in the global output options is used.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="geogenWingType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>geogenWingType</ns2:para>
					</ns2:summary>
