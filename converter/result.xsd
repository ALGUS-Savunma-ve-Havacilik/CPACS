	<xs:complexType name="loadReferenceType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>Reference values for local aerodynamic coefficients.</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:content>
							<ns2:section>
								<ns2:title>0. General overview</ns2:title>
								<ns2:content>
									<ns2:para>This type contains the reference values used for normalizing local aerodynamic coefficients. The reference point is defined in absolut CPACS-coordinates.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>1. &lt;Flightshapepoint&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>Flight shape point, specified in absolut CPACS-coordinates The reference point is used as origin for calculating the local moment coefficients. See the documentation of the global CPACS-Element for further information.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>2. &lt;Jigshapepoint&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>Jig shape point, specified in absolut CPACS-coordinates. The corredponding point on the initial CPACS geometry.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>3. &lt;area&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>Local reference area.</ns2:para>
								</ns2:content>
							</ns2:section>
							<ns2:section>
								<ns2:title>4. &lt;length&gt; <ns2:legacyItalic>(mandatory)</ns2:legacyItalic>
								</ns2:title>
								<ns2:content>
									<ns2:para>Local reference length, used for normalizing all the moment coefficients. The length is always given in the middle of the wing segment.</ns2:para>
								</ns2:content>
							</ns2:section>
						</ns2:content>
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:all>
					<xs:element name="area" type="doubleBaseType" />
					<xs:element name="length" type="doubleBaseType" />
					<xs:element name="jigShapePoint" type="pointXYZType" />
					<xs:element name="flightShapePoint" type="pointXYZType" />
				</xs:all>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>


