	<xs:complexType name="cst2DType">
		<!--****************************************************
--><xs:annotation>
			<xs:appinfo>
				<ns1:schemaDoc>
					<ns2:summary>
						<ns2:para>cst2DType</ns2:para>
					</ns2:summary>
					<ns2:remarks>
						<ns2:para />
					</ns2:remarks>
				</ns1:schemaDoc>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="complexBaseType">
				<xs:annotation>
					<xs:documentation>A 2D impelmentation for Class shape transformations. For more details look at AIAA Journal of Aircraft Vol.45 No.1 2008</xs:documentation>
				</xs:annotation>
				<xs:sequence>
					<xs:element name="psi" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>The psi vector for definition of the class and shape function, i.e. the points at which the CST functions will be evaluated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="upperN1" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>N1 for the class function for the upper side of the profile</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="upperN2" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>N2 for the class function for the upper side of the profile</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="upperB" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>B Coefficients for the Bernstein polynominal on the upper side</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lowerN1" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>N1 for the class function for the lower side of the profile</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lowerN2" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>N2 for the class function for the lower side of the profile</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="lowerB" type="stringVectorBaseType">
						<xs:annotation>
							<xs:documentation>B Coefficients for the Bernstein polynominal on the lower side</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" name="trailingEdgeThickness" type="doubleBaseType">
						<xs:annotation>
							<xs:documentation>Optionally, the trailingEdgeThickness of the profile</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

